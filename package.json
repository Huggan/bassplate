{
  "name": "bassplate",
  "version": "2.0.0",
  "description": "Boilerplate for Basscss with cssnext",
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "basscss-background-images": "latest",
    "basscss-base-buttons": "latest",
    "basscss-base-forms": "latest",
    "basscss-base-reset": "latest",
    "basscss-base-tables": "latest",
    "basscss-base-typography": "latest",
    "basscss-button-outline": "latest",
    "basscss-button-transparent": "latest",
    "basscss-color-base": "^1.1.2",
    "basscss-color-borders": "latest",
    "basscss-color-forms": "^1.1.1",
    "basscss-color-forms-dark": "^0.1.1",
    "basscss-color-tables": "latest",
    "basscss-colors": "^1.0.6",
    "basscss-defaults": "^1.0.1",
    "basscss-grid": "latest",
    "basscss-input-range": "^1.1.1",
    "basscss-positions": "latest",
    "basscss-progress": "^1.0.1",
    "basscss-table-object": "latest",
    "basscss-ui-utility-button-sizes": "latest",
    "basscss-ui-utility-groups": "latest",
    "basscss-utility-layout": "latest",
    "basscss-utility-responsive-states": "latest",
    "basscss-utility-typography": "latest",
    "basscss-utility-white-space": "latest",
    "flex-object": "latest"
  },
  "devDependencies": {
    "clean-css": "^3.1.8",
    "cssnext": "^1.1.0",
    "http-server": "^0.8.0",
    "watch": "^0.14.0"
  },
  "scripts": {
    "css": "cssnext src/css/base.css css/base.css",
    "watch:css": "watch 'npm run css' ./src/css",
    "minify:css": "cleancss css/base.css -o css/base.min.css",
    "serve": "http-server -p 8000",
    "start": "npm run watch:css & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/bassplate.git"
  },
  "keywords": [
    "CSS",
    "OOCSS",
    "Basscss",
    "cssnext",
    "Boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/jxnblk/bassplate/issues"
  },
  "homepage": "https://github.com/jxnblk/bassplate"
}
